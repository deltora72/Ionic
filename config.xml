<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.bus744120" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Bus and Train</name>
  <description>
        An Ionic Framework and Cordova project.
    </description>
  <author email="you@example.com" href="http://example.com.com/">
      Your Name Here
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <preference name="permissions" value="none"/>   
  <preference name="target-device" value="universal"/>
  <preference name="fullscreen" value="false"/>
  <preference name="webviewbounce" value="false"/>
  <preference name="orientation" value="portrait" />
  <preference name="UIWebViewBounce" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="14"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="6000"/>    
  <preference name="AutoHideSplashScreen" value="false"/>
  <preference name="ShowSplashScreenSpinner" value="false" />  
  <plugin name="org.apache.cordova.console"/>
  <plugin name="org.apache.cordova.device"/>
  <plugin name="org.apache.cordova.device-motion"/>
  <plugin name="org.apache.cordova.device-orientation"/>
  <plugin name="org.apache.cordova.dialogs"/>
  <plugin name="org.apache.cordova.splashscreen"/>
  <plugin name="org.crosswalk.engine" source="pgb" />
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="android">
    <icon src="../bus/resources/android/icon/bus-icon.png" density="ldpi"/>
    <icon src="../bus/resources/android/icon/bus-icon.png" density="mdpi"/>
    <icon src="../bus/resources/android/icon/bus-icon.png" density="hdpi"/>
    <icon src="../bus/resources/android/icon/bus-icon.png" density="xhdpi"/>
    <icon src="../bus/resources/android/icon/bus-icon.png" density="xxhdpi"/>
    <icon src="../bus/resources/android/icon/bus-icon.png" density="xxxhdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="land-ldpi"/>
    <splash src="../bus/resources/android/splash/drawable-land-mdpi-screen.png" density="land-mdpi"/>
    <splash src="../bus/resources/android/splash/drawable-land-hdpi-screen.png" density="land-hdpi"/>
    <splash src="../bus/resources/android/splash/drawable-land-xhdpi-screen.png" density="land-xhdpi"/>
    <splash src="../bus/resources/android/splash/drawable-land-xxhdpi-screen.png" density="land-xxhdpi"/>
    <splash src="../bus/resources/android/splash/drawable-land-xxxhdpi-screen.png" density="land-xxxhdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-ldpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-mdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-hdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-xhdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-xxhdpi"/>
    <splash src="../bus/resources/android/splash/999.png" density="port-xxxhdpi"/>
  </platform>
</widget>